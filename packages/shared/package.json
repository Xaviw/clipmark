{
  "name": "@clipmark/shared",
  "version": "0.1.0",
  "private": true,
  "description": "ClipMark shared types and utilities",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "linkedom": "^0.18.12",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@types/turndown": "^5.0.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
